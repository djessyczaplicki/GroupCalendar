<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GroupCalendar.View.Timetable"
      xmlns:Model="clr-namespace:GroupCalendar.Data.Remote.Model" x:Class="GroupCalendar.View.Timetable.TimetableView"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TimetableView">
    <Page.DataContext>
        <local:TimetableViewModel/>
    </Page.DataContext>
    <Grid>
        <ListView Background="#252525"
                  ScrollViewer.CanContentScroll="False"
                  ItemsSource="{Binding Events}">
        </ListView>
    </Grid>
</Page>